# Environment Configuration Example
# Copy this file to .env and update with your values
#
# If using docker-compose (recommended for local development):
#   docker-compose up -d
#   Then use the default values below (password: postgres)

# Application
NODE_ENV=development
PORT=3000
FRONTEND_URL=http://localhost:5173
LOG_LEVEL=info

# Database
# Default values match docker-compose.yml configuration
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=orchestr_ai
DB_SYNCHRONIZE=false

# Redis
# Default values match docker-compose.yml configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# JWT (optional, for future authentication)
JWT_SECRET=
